ignoreip = 10.11.8.8

[sshd]
mode   = normal
enabled 	= true
port    	= 1337
filtre		= sshd
logpath 	= /var/log/auth.log
maxretry	= 6 


[apache-dos]
enabled = true
port = http,https
filter = apache-dos
logpath = /var/log/apache2/access.log
bantime = 600
maxretry = 20
findtime = 60
action = iptables[name=HTTP, port=http, protocol=tcp]
